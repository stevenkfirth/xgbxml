	<xsd:element name="AirLoopId">
		<xsd:complexType>
			<xsd:attribute name="airLoopIdRef" type="xsd:IDREF" use="required">
				<xsd:annotation>
					<xsd:documentation>ID pointing to an air loop</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
